{
  "name": "node-stocks",
  "version": "1.0.0",
  "description": "an app I made using Node, Angular, Express, Mongo, Mongoose, Jade, and Heroku that allows a user to buy and sell stocks.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "nodemon --harmony_destructuring server.js",
    "bower": "bower",
    "postinstall": "npm run bower install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beezy12/nodeStocks.git"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true,
      "jquery": true,
      "browser": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0,
      "no-unusued-vars": 0,
      "no-undef": 0,
      "no-extra-semi": 0
    }
  },
  "author": "Brian Stumbaugh <brian.stumbaugh@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beezy12/nodeStocks/issues"
  },
  "homepage": "https://github.com/beezy12/nodeStocks#readme",
  "dependencies": {
    "body-parser": "^1.15.0",
    "bower": "^1.7.7",
    "express": "^4.13.4",
    "jade": "^1.11.0",
    "mongoose": "^4.4.4",
    "node-sass-middleware": "^0.9.7",
    "request": "^2.69.0"
  },
  "devDependencies": {}
}
